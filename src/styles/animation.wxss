.use_keyframes {
  text-align: left;
}
.use_keyframes h4,
.use_keyframes p {
  text-align: left;
}
.use_keyframes .example_img {
  display: block;
  width: 100px;
  transition: all 2s;
}
.use_keyframes table {
  display: block;
  width: 100%;
  max-height: 400px;
  border: 1px solid #aaa;
  margin: 0;
  padding: 0;
  overflow: auto;
}
.use_keyframes table th {
  border: 1px solid #aaa;
}
.use_keyframes table td {
  border: 1px solid #aaa;
}
.use_keyframes table tr:last-of-type {
  height: 200px;
}
pre {
  margin-bottom: 0;
}
.container {
  background-color: #5969d4;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .img_transform {
  flex-shrink: 0;
  width: 219px;
  height: 219px;
  overflow: hidden;
  animation: rotaimg 10s linear infinite;
  border-radius: 50%;
  position: relative;
}
.container .img_transform img {
  position: absolute;
  height: 100%!important;
  width: auto!important;
  left: 0;
  top: 0;
}
.container .icon1 {
  display: flex;
  width: 50px;
  height: 50px;
  border: 3px solid transparent;
  border-top: 3px solid #3cefff;
  border-bottom: 3px solid #3cefff;
  border-radius: 50%;
  animation: icona 1s linear infinite;
  margin: 50px;
}
.container .icon1::before {
  content: '';
  display: block;
  width: 10px;
  height: 10px;
  border: 3px solid #3cefff;
  border-radius: 50%;
  margin: auto;
  animation: iconb 2s alternate ease-in-out infinite;
}
.container .login {
  margin: 50px;
  height: 40px;
  line-height: 40px;
  width: 80px;
  background-color: #000;
  color: #fff;
  position: relative;
  overflow: hidden;
}
.container .login span:nth-of-type(1) {
  display: block;
  width: 100%;
  height: 3px;
  background: linear-gradient(to right, #fff, #3cefff);
  position: absolute;
  top: 0;
  left: 0;
  animation: login1 3s linear infinite;
}
.container .login span:nth-of-type(2) {
  display: block;
  width: 3px;
  height: 100%;
  background: linear-gradient(to bottom, #fff, #3cefff);
  position: absolute;
  right: 0;
  top: 0;
  animation: login2 3s linear infinite;
}
.container .login span:nth-of-type(3) {
  display: block;
  width: 100%;
  height: 3px;
  background: linear-gradient(to left, #fff, #3cefff);
  position: absolute;
  left: 0;
  bottom: 0;
  animation: login3 3s linear infinite;
}
.container .login span:nth-of-type(4) {
  display: block;
  width: 3px;
  height: 100%;
  background: linear-gradient(to top, #fff, #3cefff);
  position: absolute;
  left: 0;
  top: 0px;
  animation: login4 3s linear infinite;
}
.container .item {
  margin: 50px;
  width: 100px;
  height: 100px;
  border-radius: 10px;
  position: relative;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .item::before {
  content: '';
  background: conic-gradient(#fe2441 20deg, transparent 120deg);
  width: 150%;
  height: 150%;
  position: absolute;
  animation: rotate 2s linear infinite;
}
.container .item::after {
  content: '测试';
  width: 90px;
  height: 90px;
  background-color: #101010;
  position: absolute;
  border-radius: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fe2441;
}
.container .item:hover::before {
  animation-play-state: paused;
}
.container .light_ball {
  margin: 50px;
  width: 100px;
  height: 100px;
  border-radius: 50%;
  box-shadow: 0 0 20px #fff, -20px 0 80px #f0f, 20px 0 80px #0ff, inset 0 0 50px #fff, inset 50px 0 80px #f0f, inset -50px 0 80px #0ff, inset 50px 0 200px #f0f, inset -50px 0 200px #0ff;
  animation: balla 3s linear infinite;
}
.container .nxhz {
  width: 100px;
  height: 100px;
  background-color: #5969d4;
  filter: contrast(50);
}
.container .nxhz .box1,
.container .nxhz .box2 {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background-color: white;
  filter: blur(25px);
}
.container .nxhz .box1 {
  animation: nxhzdh 3s linear infinite;
}
.container .nxhz .box2 {
  animation: nxhzdh2 3s linear infinite;
}
@keyframes nxhzdh {
  from {
    transform: translateX(-30px);
  }
  to {
    transform: translateX(25px);
  }
}
@keyframes nxhzdh2 {
  from {
    transform: translateX(30px);
  }
  to {
    transform: translateX(-25px);
  }
}
@keyframes balla {
  50% {
    box-shadow: 0 0 20px #fff, 20px 0 80px #f0f, -20px 0 80px #0ff, inset 0 0 50px #fff, inset -50px 0 80px #f0f, inset 50px 0 80px #0ff, inset -50px 0 200px #f0f, inset 50px 0 200px #0ff;
  }
}
@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
@keyframes login1 {
  from {
    transform: translateX(-80px);
  }
  to {
    transform: translateX(80px);
  }
}
@keyframes login2 {
  from {
    transform: translateY(-40px);
  }
  to {
    transform: translateY(40px);
  }
}
@keyframes login3 {
  from {
    transform: translateX(80px);
  }
  to {
    transform: translateX(-80px);
  }
}
@keyframes login4 {
  from {
    transform: translateY(40px);
  }
  to {
    transform: translateY(-40px);
  }
}
@keyframes icona {
  to {
    transform: rotate(360deg);
  }
}
@keyframes iconb {
  from {
    transform: scale(0.5);
  }
  to {
    transform: scale(1.2);
  }
}
@keyframes rotaimg {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
